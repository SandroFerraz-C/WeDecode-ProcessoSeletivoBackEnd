datasource db {
  provider = "mysql"
  url  = env("DATABASE_URL")
}

generator films {
  provider = "prisma-client-js"
}



model Films {
  id                                  Int     @id @default(autoincrement())
  Filme                           String @unique
  Genero                         String
  DataDeLancamento    String
  Classificacao               String
  TempoDeFilme           String
  Diretor                        String  
  Orcamento                  String


  EspectadorFilmes Espectador []
}

model Espectador {
  id                                Int               @id @default(autoincrement())

  EspectadorFilmes    Films    @relation(fields: [EspectadorId], references: [id])
  EspectadorId             Int

  Nome                        String
  Documento               Int
 
}
